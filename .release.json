{
    "non-interactive": false,
    "dry-run": false,
    "verbose": false,
    "force": false,
    "pkgFiles": ["package.json"],
    "increment": "patch",
    "prereleaseId": null,
    "commitMessage": "Release v%s",
    "tagName": "v%s",
    "tagAnnotation": "Release v%s",
    "buildCommand": false,
    "changelogCommand": "npm run changelog",
    "requireCleanWorkingDir": false,
    "src": {
        "pushRepo": null,
        "beforeStartCommand": false,
        "beforeStageCommand": false,
        "afterReleaseCommand": false
    },
    "dist": {
        "repo": false,
        "stageDir": ".stage",
        "baseDir": "dist",
        "files": ["**/*"],
        "pkgFiles": null,
        "beforeStageCommand": false,
        "afterReleaseCommand": false
    },
    "npm": {
        "publish": false,
        "publishPath": ".",
        "tag": "latest",
        "private": true,
        "forcePublishSourceRepo": false
    },
    "github": {
        "release": true,
        "releaseName": "Release v%s",
        "tokenRef": "GITHUB_TOKEN"
    }
}
