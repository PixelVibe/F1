  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PixelVibe/F1"
  },
  "keywords": [
